                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/Bytes"
                                }
                            ]
                        },
                        "runtime": {
                            "description": "The execution runtime to use.",
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/TransactionRuntime"
                                }
                            ]
