{
    "description": "Execution target of a Transaction.",
    "oneOf": [
        {
            "description": "The execution target is a native operation (e.g. a transfer).",
            "type": "string",
            "enum": [
                "Native"
            ]
        },
        {
            "description": "The execution target is a stored entity or package.",
